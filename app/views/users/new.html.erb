New User

<%= form_for @user do |f| %>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :photo %>
        <%= f.text_field :photo %>
    </div>
    <div>
        <%= f.label :bio %>
        <%= f.text_field :bio %>
    </div>
    <div>
        <%= f.submit "Add a user" %>
    </div>


<% end %>
